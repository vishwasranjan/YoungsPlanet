<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.prabisha.youngsplanet">

    <application
        android:allowBackup="true"
        android:icon="@drawable/green_toys_planet_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.YoungsPlanet">
        <activity android:name=".PaymentActivity"></activity>
        <activity android:name=".CartActivity" />
        <activity
            android:name=".AccountsActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity android:name=".ClothesActivity" />
        <activity android:name=".BooksActivity" />
        <activity android:name=".ToyActivity" />
        <activity android:name=".StoreActivity" />
        <activity android:name=".HomeActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar"/>
        <activity
            android:name=".RegisterActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity
            android:name=".LoginActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>